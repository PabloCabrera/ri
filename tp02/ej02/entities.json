{
	"abbr": "[A-Z][a-z]{0,3}\\.|[A-Z]{2,6}(?:[ .,;])|([A-Z]\\.)+",
	"email": "[a-zA-Z0-9._\\-]+@[a-zA-Z0-9._\\-]+\\.[a-zA-Z0-9._\\-]+",
	"url": "[a-zA-Z][a-zA-Z0-9]*://[a-zA-Z0-9._\\-@:]+/([^ ]*)?",
	"phone_number": "[+]?([(]?[0-9]+[)]?[ \\-]?){5,}",
	"decimal_number": "[0-9]+\\.[0-9]+",
	"integer_number": "[0-9]+",
	"html_color_code": "#([0-9a-fA-F]{3}){1,2}",
	"name": "[A-Z][a-z]+( [A-Z][a-z]+)+"
}
